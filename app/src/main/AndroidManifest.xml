<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.overall.developer.overrendicion">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".RendicionApplication"
        android:allowBackup="true"
        android:icon="@drawable/rendicionicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.user.view.Login.LoginActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.liquidacion.view.pendiente.PendienteActivity"/>
        <activity
            android:name=".ui.user.view.CreateAccount.CreateAccountActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen" />
        <activity android:name=".ui.liquidacion.view.rendicion.RendicionActivity" />
        <activity android:name=".ui.liquidacion.view.formularios.FormularioActivity" />
        <activity android:name=".ui.liquidacion.view.datosGenerales.DatosGeneralesActivity" />

    </application>

</manifest>