<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.overall.developer.overrendicion">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".RendicionApplication"
        android:allowBackup="true"
        android:icon="@drawable/rendicionicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.user.view.Login.LoginActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.liquidacion.view.pendiente.PendienteActivity" />
        <activity
            android:name=".ui.user.view.CreateAccount.CreateAccountActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen" />
        <activity android:name=".ui.liquidacion.view.rendicion.RendicionActivity" />
        <activity android:name=".ui.liquidacion.view.formularios.FormularioActivity" />
        <activity android:name=".ui.liquidacion.view.datosGenerales.DatosGeneralesActivity" />

        <service
            android:name="com.amazonaws.mobileconnectors.s3.transferutility.TransferService"
            android:enabled="true" />

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="d5ba6603f446f535647ee0629e0ca511ed970e4f" />

        <service
            android:name=".ui.liquidacion.interactor.Rendicion.SendDataService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".ui.user.view.Drawable.RecoveryPasswordActivity" />
        <activity android:name=".ui.user.view.Drawable.UpdateEmailActivity" />
        <activity android:name=".ui.liquidacion.view.rendicion.contentFoto.FotoContentActivity"></activity>
    </application>

</manifest>